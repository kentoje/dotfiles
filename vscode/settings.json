{
    "launch": {
        "configurations": [],
        "compounds": []
    },
    // Editor
    "editor.quickSuggestions": true,
    "editor.suggestOnTriggerCharacters": true,
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": true
    },
    "editor.fontFamily": "SF Mono",
    "editor.fontWeight": "600",
    "editor.fontLigatures": true,
    "editor.fontSize": 16,
    "editor.tabSize": 2,
    "editor.lineHeight": 30,
    "editor.cursorBlinking": "expand",
    // Files
    "files.autoSave": "onFocusChange",
    "files.trimTrailingWhitespace": true,
    // Workbench
    "workbench.colorTheme": "Gruvbox Dark Soft",
    "workbench.iconTheme": "vscode-icons",
    "workbench.editor.enablePreview": false,
    "workbench.colorCustomizations": {
        "tab.activeBackground": "#2c3643",
        "activityBar.background": "#282828",
        "sideBar.background": "#3C3836",
        "input.background": "#242D38",
        "input.border": "#37404E",
        "list.focusBackground": "#215D8C",
        "list.focusForeground": "#FB4934",
        "tab.inactiveBackground": "#242D38",
        "editorGroupHeader.tabsBackground": "#212934",
        "editor.selectionBackground": "#504945",
        "editorLineNumber.activeForeground": "#FB4934",
        "titleBar.activeBackground": "#242D38",
        "statusBar.background": "#3b444f",
        "editorCursor.foreground": "#8ec07c",
    },
    // Window
    "window.zoomLevel": 0,
    // Emmet
    "emmet.includeLanguages": {
        "javascript": "javascriptreact",
        "twig": "html"
    },
    "[go]": {
        "editor.tabSize": 4
    },
    // Tabnine
    "tabnine.experimentalAutoImports": true,
    "editor.renderWhitespace": "boundary",
    "editor.formatOnSave": true,
    "prettier.requireConfig": true,
    "python.formatting.provider": "none",
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    // Vim
    // "vim.handleKeys": {
    //     "<C-c>": false,
    //     "<C-f>": false,
    //     "<C-d>": false,
    //     "<C-a>": false,
    //     "<C-x>": false,
    //     "<C-w>": false,
    //     "<C-k>": false,
    //     "<C-z>": false,
    //     "<C-b>": false
    // }
}


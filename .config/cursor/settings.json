{
  "extensions.experimental.affinity": {
    "asvetliakov.vscode-neovim": 1
  },
  "files.autoSave": "off",
  "explorer.sortOrder": "type",
  "explorer.autoReveal": false,
  "editor.formatOnSave": true,
  "html.autoClosingTags": false,
  "typescript.autoClosingTags": false,
  "javascript.autoClosingTags": false,
  "editor.autoClosingBrackets": "never",
  "search.searchEditor.focusResultsOnSearch": false,
  "search.collapseResults": "alwaysCollapse",
  "editor.stickyScroll.enabled": true,
  "editor.minimap.enabled": false,
  "editor.cursorBlinking": "phase",
  "editor.overviewRulerBorder": false,
  "editor.padding.bottom": 16,
  "editor.padding.top": 16,
  "editor.scrollbar.horizontal": "hidden",
  "editor.lineNumbers": "relative",
  "editor.fontSize": 14,
  "editor.lineHeight": 1.5,
  "editor.cursorSurroundingLines": 15,
  "editor.fontFamily": "CommitMono Nerd Font",
  "breadcrumbs.enabled": false,
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "workbench.sideBar.location": "right",
  "window.title": " ",
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "eslint.format.enable": true,
  "workbench.colorTheme": "Oscura Midnight",
  "workbench.colorCustomizations": {
    "list.focusBackground": "#6DBC9966",
    "list.focusForeground": "#FFFFFF",
    "quickInputList.focusBackground": "#6DBC9966",
    "editorCursor.foreground": "#A052FF",
    "editorCursor.background": "#FFF",
    "editorLineNumber.activeForeground": "#A052FF",
    "titleBar.activeBackground": "#000000",
    "titleBar.activeForeground": "#222",
    "titleBar.inactiveBackground": "#000000",
    "titleBar.inactiveForeground": "#222",
    "editor.selectionForeground": "#6DBC9966",
    "menu.selectionForeground": "#6DBC9966",
    "list.activeSelectionBackground": "#6DBC9966",
    "list.activeSelectionForeground": "#FFFFFF"
  },
  "editor.accessibilitySupport": "off",
  "editor.autoClosingQuotes": "never",
  "workbench.iconTheme": "icons",
  "workbench.editor.enablePreview": false,
  "workbench.editor.showTabs": "single",
  "vscode_custom_css.imports": [
    "file://$HOME/dotfiles/.config/cursor/js/cursor-animation.js"
  ]
}
